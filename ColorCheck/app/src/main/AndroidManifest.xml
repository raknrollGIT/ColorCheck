<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="git_lab.colorcheck">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Menu">
            <intent-filter>
                <action android:name=".Menu" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".jezyki">
            <intent-filter>
                <action android:name=".jezyki" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Tworcy" />
        <activity android:name=".Game" />
        <activity android:name=".Ranking" />
        <activity android:name=".HowTo" />
        <activity android:name=".Trudnosc" />
        <activity android:name=".Global" />
        <activity android:name=".gameOver" />
        <activity android:name=".Baza" />
        <activity android:name=".BazaV2"></activity>
    </application>

</manifest>